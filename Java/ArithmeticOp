public class ArithmeticOp {
    public static void main(String[] args) {
        int a=2, b =3;
        System.out.println("Sum is"+ (a+b));
        System.out.println("a-b is"+ (a-b));
        System.out.println("a*b is"+ (a*b));
        System.out.println("a/b is"+ (a/b));
        System.out.println("a%b is"+ (a%b));
    }
}
